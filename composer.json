{
    "name": "xiian/bundleroute",
    "description": "Reference local Symfony bundle routes in your twig templates regardless of prefixing",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Tom Sartain",
            "email": "tomsartain@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/dependency-injection": "^7.2",
        "symfony/http-kernel": "^7",
        "symfony/routing": "^7",
        "twig/twig": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Xiian\\BundleRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xiian\\BundleRoute\\Test\\": "tests/"
        }
    }
}
